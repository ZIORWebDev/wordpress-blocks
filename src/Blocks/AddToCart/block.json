{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "zior/add-to-cart",
	"title": "Add to Cart",
	"category": "design",
	"description": "Display WooCommerce add to cart button.",
	"textdomain": "default",
	"attributes": {
		"productId": {
			"type": "string"
		},
		"showQuantity": {
			"type": "boolean",
			"default": false
		}
	},
	"supports": {
		"align": true
	},
	"editorStyle": "wp-block-zior-add-to-cart-editor"
}